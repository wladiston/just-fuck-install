{
  "name": "just-fucking-install",
  "description": "A package manager interface for npm, yarn, and pnpm",
  "version": "0.0.1",
  "type": "module",
  "exports": "./dist/index.js",
  "bin": {
    "jf": "./dist/cli.js"
  },
  "scripts": {
    "dev": "node ./scripts/build.cjs",
    "buid": "tsc && node ./scripts/build.cjs",
    "test": "vitest",
    "format": "prettier --write ."
  },
  "devDependencies": {
    "@types/node": "^18.8.4",
    "esbuild": "^0.15.10",
    "prettier": "^2.7.1",
    "typescript": "^4.6.4",
    "vitest": "^0.24.1"
  },
  "keywords": [
    "package",
    "manager",
    "npm",
    "yarn",
    "pnpm"
  ],
  "packageManager": "pnpm@7.13.4",
  "author": "Wlad Paiva <desenvolvedormg@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wladiston/my_package/issues"
  },
  "homepage": "https://github.com/wladiston/my_package",
  "repository": {
    "type": "git",
    "url": "https://github.com/wladiston/my_package.git"
  },
  "dependencies": {
    "@types/events": "^3.0.0",
    "cac": "^6.7.14",
    "chalk": "^5.1.2"
  }
